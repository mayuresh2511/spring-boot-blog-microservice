keytool -genkeypair -alias springbootssl -keyalg RSA -keysize 2048 -storetype PKCS12 -keystore D:\JavaProjects\Microservices\user-service\src\main\resources\keystore\springbootssl.p12 -validity 3650
keytool -list -keystore  springbootssl.p12
keytool -export -alias springbootssl -keystore springbootssl.p12 -rfc -file D:\JavaProjects\Microservices\user-service\src\main\resources\springbootssl.p12